<?xml version="1.0" encoding="UTF-8"?>

<webServices xmlns="http://java.sun.com/xml/ns/jax-rpc/ri/dd"
    version="1.0"
    targetNamespaceBase="http://opc.blueprints.j2ee.sun.com/wsdl"
    typeNamespaceBase="http://opc.blueprints.j2ee.sun.com/types"
    urlPatternBase="/webservice">

    <endpoint name="OPCWebService"
        displayName="OPC Web Service"
        description="Order Processing Center Web Service" 
        interface="com.sun.j2ee.blueprints.opc.webservice.OPCService"  
        implementation="com.sun.j2ee.blueprints.opc.webservice.OPCServiceImpl"/> 
 
    <endpointMapping
        endpointName="OPCWebService"
        urlPattern="/webservice/OPCService"/>

</webServices>


