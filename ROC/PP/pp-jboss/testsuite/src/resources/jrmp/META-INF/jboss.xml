<?xml version="1.0"?>

<!-- The jboss.xml descriptor for the jrmp-comp.jar ejb unit -->
<jboss>
    <enterprise-beans>
        <session>
            <ejb-name>StatelessSession</ejb-name>
            <configuration-name>Standard Stateless SessionBean</configuration-name>
            <home-invoker>jboss:service=invoker,type=jrmp,socketType=CompressionSocketFactory</home-invoker>
            <bean-invoker>jboss:service=invoker,type=jrmp,socketType=CompressionSocketFactory</bean-invoker>
        </session>
    </enterprise-beans>
</jboss>
